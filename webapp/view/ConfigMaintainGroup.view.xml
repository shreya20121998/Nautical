<mvc:View
    controllerName="nauticalfe.controller.ConfigMaintainGroup"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
 
>
    <Page id="page">
        <headerContent>
            <!-- Your header content goes here -->
            <Bar id="_IDGenBar2" class="customHeader">
                <contentLeft>
                    <Button id="_IDGenButton1" class="customHeaderButton"  icon="sap-icon://nav-back" type="Default" />
                    <Image id="_IDGenImage1" src="../images/logo.jpg" height="75%" />
                    <Text id="_IDGenText1" class="textBid" text="Change View User ID and Group Mapping: Overview" />
                </contentLeft>

                <contentRight>
                    <core:Icon id="_IDGenIcon1" src="sap-icon://search" class="size1" color="#FFFFFF">
                        <core:layoutData>
                            <FlexItemData id="_IDGenFlexItemData1" growFactor="1" />
                        </core:layoutData>
                    </core:Icon>

                    <core:Icon id="_IDGenIcon2" src="sap-icon://SAP-icons-TNT/user" class="size1" color="#FFFFFF" >
                        <core:layoutData>
                            <FlexItemData id="_IDGenFlexItemData2" growFactor="1"/>
                        </core:layoutData>
                    </core:Icon>
                </contentRight>
            </Bar>
        </headerContent>
        <Bar id="_IDGenBar1" class="_IDGenBar1" >
            <contentLeft>
                <Button id="_IDGenButton2" text="New Entries" press="onCreatePress" />
                <Button id="_IDGenButton3" text="Copy As" />
                <Button id="_IDGenButton4"  text="Delete" />
                <Button id="_IDGenButton5" text="Undo Change" />
                <Button id="_IDGenButton6" text="Select All" />
                <Button id="_IDGenButton7" text="Select Block" />
                <Button id="_IDGenButton8" text="Deselect All" />
                <Button id="button" icon="sap-icon://slim-arrow-down" iconFirst="false" text="More" press="onPress"
                    ariaHasPopup="Menu"/>
            </contentLeft>

            <contentRight>
                <core:Icon id="_IDGenIcon3" src="sap-icon://print" class="size1" color="#FF0000" />
                <Button id="_IDGenButton9" text="Display" />
                <Button id="_IDGenButton10" text="Exit" press="MastPage" />
            </contentRight>
        </Bar>
        <Text id="_IDGenText01" text="User ID and Group Mapping " class="masterheadings"/>
        <Table width="100%" class="mastertables" visible="true" mode="MultiSelect"
            id="firstTableId">
            <columns>
                <!-- <Column id="_IDGenColumn1" width="30px">
                    <Label id="_IDGenLabel1" text="" />
                </Column> -->
                <Column id="_IDGenColumn2" width="10%">
                    <Label id="_IDGenLabel2" text="User Name" />
                </Column>
                <Column id="_IDGenColumn3" width="15%">
                    <Label id="_IDGenLabel3" text="User ID Group" />
                </Column>
                <Column id="_IDGenColumn4">
                    <Label id="_IDGenLabel4" text="" />
                </Column>
            </columns>
            <ColumnListItem id="_IDClmnListItem1">
                <cells>
                    <Text id="_IDGenTxt2" text="{}"></Text>
                    <Text id="_IDGenTxt3" text="{}"></Text>
                    <Text id="_IDGenTxt4" text="{}"></Text>
                </cells>
            </ColumnListItem>
            <ColumnListItem id="_IDGenClmnListItem1">
                <cells>
                    <Text id="_IDText2" text="{}"></Text>
                    <Text id="_IDText3" text="{}"></Text>
                    <Text id="_IDText4" text="{}"></Text>
                </cells>
            </ColumnListItem>
            <ColumnListItem id="_IDGenColmnListItem1">
                <cells>
                    <Text id="_IDTxt2" text="{}"></Text>
                    <Text id="_IDTxt3" text="{}"></Text>
                    <Text id="_IDTxt4" text="{}"></Text>
                </cells>
            </ColumnListItem>
            <ColumnListItem id="_IDGenColumnListItem1">
                <cells>
                    <Text id="IDTxt0" text="{}"></Text>
                    <Text id="IDGenText3" text="{}"></Text>
                    <Text id="IDGenText4" text="{}"></Text>
                </cells>
            </ColumnListItem>
            <ColumnListItem id="_IDColumnListItem1">
                <cells>
                    <Text id="_IDGenText2" text="{}"></Text>
                    <Text id="_IDGenText3" text="{}"></Text>
                    <Text id="_IDGenText4" text="{}"></Text>
                </cells>
            </ColumnListItem>
        </Table>
 
        <Bar >
            <contentRight>
            <VBox id="toHide">
                <HBox id="_IDGenHBox1">
                    <Button id="_IDGenButton11" text="Save" />
                    <Button id="_IDGenButton12"
                        text="Cancel"
                        press="MastPage"
                    />
                </HBox>
            </VBox>
            </contentRight>
        </Bar>
       
    </Page>
</mvc:View>