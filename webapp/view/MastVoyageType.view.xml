<mvc:View
    xmlns:tnt="sap.tnt"
    xmlns:form="sap.ui.layout.form"
    xmlns:l="sap.ui.layout"
    controllerName="nauticalfe.controller.MastVoyageType"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:core="sap.ui.core"
>
    <Page id="_IDGenPage1">
        <headerContent>
            <!-- Your header content goes here -->
            <Bar class="customHeader">
                <contentLeft>
                    <Button
                        class="customHeaderButton"
                        icon="sap-icon://nav-back"
                        press="backPress"
                    />
                    <Image
                        src="../images/ignxlogo.png"
                        height="2rem"
                        width="5rem"
                    />
                    <Text
                        class="textBid"
                        text="Change View Voyage Type Master Table: Overview"
                    />
                </contentLeft>
                <contentRight>
                    <core:Icon
                        src="sap-icon://search"
                        class="size1"
                        color="#FFFFFF"
                    >
                        <core:layoutData>
                            <FlexItemData growFactor="1" />
                        </core:layoutData>
                    </core:Icon>
                    <core:Icon
                        src="sap-icon://person-placeholder"
                        class="size1"
                        color="#FFFFFF"
                    >
                        <core:layoutData>
                            <FlexItemData growFactor="1" />
                        </core:layoutData>
                    </core:Icon>
                </contentRight>
            </Bar>
        </headerContent>
        <content>
            <!-- <ObjectHeader
                id="_IDGenObjectHeader1"
                title="Voyage Charter Type Screen"
                icon="sap-icon://form"
            /> -->
            <Panel id="_IDGenPanel1">
                <Dialog
                    id="helloDialog"
                    resizable="true"
                    draggable="true"
                    title="Create Charter Type"
                >
                    <VBox
                        id="_IDGenVBox1"
                        class="dialogContent_Vbox"
                    >
                        <Label
                            id="_IDGenLabel1"
                            labelFor="charterCode"
                            text="Charter Code"
                        />
                        <Input
                            id="charterCode"
                            placeholder="code"
                            width="190px"
                            maxLength="4"
                        />
                        <Label
                            id="_IDGenLabel2"
                            labelFor="charterDesc"
                            text="Charter Description"
                        />
                        <Input
                            id="charterDesc"
                            placeholder="description"
                            width="190px"
                        />
                    </VBox>

                    <footer>
                        <Toolbar id="_IDGenToolbar2">
                            <ToolbarSpacer id="_IDGenToolbarSpacer2" />
                            <Button
                                id="_IDGenButton10"
                                type="Emphasized"
                                press="onCreateEntry"
                                text="create"
                                class="sapUiTinyMarginTop sapUiTinyMarginBottom"
                            />
                            <Button
                                id="_IDGenButton9"
                                type="Reject"
                                press=".handleCloseCreate"
                                text="cancel"
                                class="sapUiTinyMarginTop sapUiTinyMarginBottom"
                            />
                        </Toolbar>
                    </footer>
                </Dialog>
                <Dialog
                    id="helloDialog2"
                    resizable="true"
                    draggable="true"
                    title="Update Charter Type"
                >
                    <VBox
                        id="_IDGenVBox2"
                        class="dialogContent_Vbox"
                    >
                        <Label
                            id="_IDGenLabel12"
                            labelFor="charterCode2"
                            text="Charter Code"
                        />
                        <Input
                            id="charterCode2"
                            placeholder="code"
                            width="190px"
                            maxLength="4"
                            editable="false"
                        />
                        <Label
                            id="_IDGenLabel3"
                            labelFor="charterDesc2"
                            text="Charter Description"
                        />
                        <Input
                            id="charterDesc2"
                            placeholder="description"
                            width="190px"
                        />
                    </VBox>

                    <footer>
                        <Toolbar id="_IDGenToolbar1">
                            <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                            <Button
                                id="_IDGenButton7"
                                type="Emphasized"
                                press=".onUpdate"
                                text="update"
                                class="sapUiTinyMarginTop sapUiTinyMarginBottom"
                            />
                            <Button
                                id="_IDGenButton8"
                                type="Reject"
                                press=".handleCloseUpdate"
                                text="cancel"
                                class="sapUiTinyMarginTop sapUiTinyMarginBottom"
                            />
                        </Toolbar>
                    </footer>
                </Dialog>
                
                <Bar
                    id="_IDGenBar1"
                    class="_IDGenBar1"
                >
                    <contentLeft>
                        <Button
                            text="New Entries"
                            press="onShowDialogForCreate"
                        />
                        <Button text="Copy As" />
                        <Button text="Delete" press="onDeleteVoyageType" />
                        <Button text="Update" press="onShowUpdateDialog" />
                        <Button text="Select All" />
                        <Button text="Select Block" />
                        <Button text="Deselect All" />
                        <Button
                            id="button"
                            icon="sap-icon://slim-arrow-down"
                            iconFirst="false"
                            text="More"
                            press="onPress"
                            ariaHasPopup="Menu"
                        />
                    </contentLeft>
                    <contentRight>
                        <core:Icon
                            src="sap-icon://print"
                            class="size1"
                            color="#FF0000"
                        />
                        <Button text="Display" />
                        <Button
                            text="Exit"
                            press="onExit"
                        />
                    </contentRight>
                </Bar>
                <Table
                    id="createTypeTable"
                    mode="MultiSelect"
                    items="{/VoyTypeSet}"
                    updateFinished="onTableUpdateFinished"
                    selectionChange="selectedItems"
                    width="100%"
                >
                    <columns>
                        <Column id="_IDGenColumn1">
                            <Text
                                id="_IDGenText1"
                                text="Voyage Charter Code"
                            />
                        </Column>
                        <Column id="_IDGenColumn2">
                            <Text
                                id="_IDGenText2"
                                text="Charter Description"
                            />
                        </Column>
                    </columns>
                    <items>
                        <ColumnListItem id="_IDGenColumnListItem1">
                            <cells>
                                <Text
                                    id="_IDGenInput1"
                                    text="{Voycd}"
                                    
                                />
                                <Text
                                    id="_IDGenInput2"
                                    text="{Voydes}"
                                    
                                />
                                <Button
                                    id="_IDGenButton1"
                                    icon="sap-icon://delete"
                                    press="onDeleteRow"
                                />
                            </cells>
                        </ColumnListItem>
                    </items>
                </Table>
                <!-- <Button id="_IDGenButton2" text="Add Row" press="updateTableRefresh" /> -->
            </Panel>
        </content>
        <footer>
            <OverflowToolbar design="Auto">
            <ToolbarSpacer />
            <Button text="Save" type="Neutral"></Button>
            <Button text="Cancel" type="Negative" ></Button>
            </OverflowToolbar>
        </footer>
    </Page>

    <!--scrollbar added-->
    <ScrollContainer
        id="_IDGenScrollContainer1"
        height="100%"
        width="100%"
        vertical="true"
        focusable="true"
    >
        <!--this is first page-->
    </ScrollContainer>
</mvc:View>