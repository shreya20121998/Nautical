<mvc:View xmlns:tnt="sap.tnt"
xmlns:form="sap.ui.layout.form"
xmlns:l="sap.ui.layout"
controllerName="nauticalfe.controller.dash"
xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" displayBlock="true">
    
   <Page id="_IDGenPage1" title="Nautical Dashboard" >
      <VBox id="_IDGenVBox1" class="vPanel">
         <Panel id="_IDGenPanel1" class="panel" width="700px" height="auto">
            
            <ObjectListItem id="_IDGenObjectListItem1" intro="Select Transaction Type" ></ObjectListItem>

            <Button id="_IDGenButton1" text="Transaction Data" press="TransData" class="Trans" width="40%" icon="sap-icon://request"></Button>
         
            <Button id="_IDGenButton2" text="Master Data" press="MastData" class="Mastr" width="40%" icon="sap-icon://visits"></Button>
         </Panel>   
            
         <Panel id="_IDGenPanel2"  class="panel1">
            <Image id="_IDGenImage1" class="mastrimg" src="../images/nautical.png" width="350px" height="auto"   ></Image>
         </Panel>
      </VBox>
      <footer>
         <OverflowToolbar >
         </OverflowToolbar>
      </footer>
   </Page>
   
    

</mvc:View> 
