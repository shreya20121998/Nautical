    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="businessPartnerPageId" title="Business Partner">
        <VBox id="_IDGenVBox1" class="vPanel">

            <Panel id="_IDGenPanel1" class="panel" width="650px" height="650px">
                <ObjectListItem id="_IDGenObjectListItem1" intro="Select Options" ></ObjectListItem>

                <Button id="_IDGenButton1" text="BP Master Details" class="Trans" width="30%"
                 icon="sap-icon://request" press="onBPDetailpress"></Button>

                <Button id="_IDGenButton2" text="Vendor Data Syncing" press="MastData" class="Mastr" width="30%" icon="sap-icon://visits"></Button>
            </Panel>   
        </VBox>
    </Page>
</mvc:View>
