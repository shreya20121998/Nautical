<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog id="fileUpload" class="sapUiContentPadding" contentWidth="400px" contentHeight="130px" title="Chartering Request" >
        <content>
            <Text id="_IDGenText1" text="This application needs a file to upload to SAP System"></Text>
                <HBox id="_IDGenHBox1" class="MastDialogButtonAlign">
                <Button id="_IDGenButton1" text="Ok" class="masterbuttonsalign"></Button>
                <Button id="_IDGenButton2" text="Exit" class="masterbuttonsalign" press="exitDialog"></Button>
            </HBox>
        </content>
    </Dialog>
</core:FragmentDefinition>